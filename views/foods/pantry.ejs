<%- include('../partials/_navbar.ejs') %>

<h1>Pantry</h1>

<% pantry.forEach(food => { %>
  <div style="border: 1px solid #ccc; padding: 15px; margin-bottom: 15px;">
    <h3><%= food.title %></h3>
    <p><%= food.recipe %></p>
    <a href="/users/<%= user._id %>/foods/<%= food._id %>">Show other users foods</a>

  </div>

<% }) %>
